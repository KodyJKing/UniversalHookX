push rbp
lea rbp, [rsp+08]

// Save rflags
pushfq

// Save volatile registers. See [0].
push rax
push rcx
push rdx
push r8
push r9
push r10
push r11

// Align stack to 16 bytes
mov rbx, rsp
and rsp, 0xfffffffffffffff0

// Save volatile xmm registers. See [0].
sub rsp, 60
movdqa [rsp], xmm0
movdqa [rsp+10], xmm1
movdqa [rsp+20], xmm2
movdqa [rsp+30], xmm3
movdqa [rsp+40], xmm4
movdqa [rsp+50], xmm5